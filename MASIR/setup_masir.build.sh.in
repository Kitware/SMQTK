# Setup script for running MASIR web-app out of the build directory.

export PYTHONPATH=@MASIR_SOURCE_DIR@/python:@MASIR_BINARY_DIR@/python:@TPL_PYTHON_SP@:$PYTHONPATH
export PATH=@MASIR_SOURCE_DIR@/bin:@MASIR_BINARY_DIR@/bin:@TPL_INSTALL_PREFIX@/bin:$PATH
export LD_LIBRARY_PATH=@TPL_INSTALL_PREFIX@/lib:$LD_LIBRARY_PATH

export MASIR_INSTALL_DIR="@MASIR_SOURCE_DIR@"
export MASIR_SETUP=1
